    <style name="Theme.JamRSS.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.JamRSS.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>